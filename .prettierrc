{
	"tabWidth": 2,
	"useTabs": false,
	"semi": true,
	"singleQuote": false,
	"trailingComma": "all",
	"bracketSpacing": true,
	"jsxBracketSameLine": true,
	"arrowParens": "always",
	"endOfLine": "auto",
	"quoteProps": "as-needed",
	"jsxSingleQuote": false,
	"proseWrap": "preserve",
	"embeddedLanguageFormatting": "auto",
	"printWidth": 80,
	"plugins": [
		"prettier-plugin-astro",
		"prettier-plugin-tailwindcss"
	],
	"overrides": [
		{
			"files": "*.astro",
			"options": {
				"parser": "astro"
			}
		}
	]
}